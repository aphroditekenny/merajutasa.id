{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Evidence Collision Test Report v1",
  "type": "object",
  "required": ["version","generated_utc","prefix_length","artifact_hashes","collision_count","collisions"],
  "properties": {
    "version": {"type":"string"},
    "generated_utc": {"type":"string"},
    "prefix_length": {"type":"number"},
    "artifact_hashes": {"type":"number"},
    "collision_count": {"type":"number"},
    "collisions": {"type":"array", "items": {"type":"object", "properties": {"prefix": {"type":"string"}, "first": {"type":"object"}, "second": {"type":"object"}}, "required": ["prefix","first","second"]}}
  }
}
